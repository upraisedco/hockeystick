{
  "name": "hockeystick",
  "sideEffects": false,
  "version": "0.0.1",
  "description": "Frontend for Upraised",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack -p --config ./webpack/prod.config.js --progress --colors --optimize-minimize --optimize-dedupe",
    "build-stage": "./node_modules/.bin/webpack -p --config ./webpack/stage.config.js --progress --colors --optimize-minimize --optimize-dedupe",
    "dev": "./node_modules/.bin/webpack-dev-server --config ./webpack/dev.config.js --watch",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/upraisedco/hockeystick.git"
  },
  "keywords": [
    "react",
    "redux",
    "saga",
    "webpack"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/upraisedco/hockeystick/issues"
  },
  "homepage": "https://github.com/upraisedco/hockeystick#readme",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "bootstrap": "^4.1.1",
    "cors": "^2.8.4",
    "npm": "^5.6.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-fontawesome": "^1.6.1",
    "react-load-script": "0.0.6",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^6.1.0",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "webpack": "^4.6.0",
    "webpack-dev-server": "~3.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "jest": "^23.1.0",
    "json-loader": "^0.5.7",
    "less": "^3.0.2",
    "less-loader": "^4.1.0",
    "node-sass": "^4.9.2",
    "normalize.css": "^8.0.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack-cli": "^3.1.1",
    "webpack-merge": "^4.1.4"
  }
}
